[alias]
    churn = "!git rev-list --objects --all | awk '$2' | sort -k2 | uniq -cf1 | sort -rn"

[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true

[core]
	ignorecase = true
	autocrlf = input
	excludesfile = /Users/Gufran/.gitignore_global
	pager = less -x4

